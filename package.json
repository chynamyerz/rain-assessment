{
  "name": "rain-assessment",
  "private": true,
  "scripts": {
    "frontend": "cd apps/frontend && npm run dev",
    "frontend:test": "cd apps/frontend && npm run test",
    "backend": "cd apps/backend && npm run dev",
    "backend:test": "cd apps/backend && npm run test",
    "backend:db:migrate": "cd apps/backend && npm run migrate",
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "test": "turbo test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.0.6",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.5.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "classnames": "^2.5.1"
  }
}
